.goods-container{
  position: relative;
  float: left;
  width: 46%;
  height: 452rpx;
  background: #fff;
  margin: 20rpx 0 0 20rpx;
  border: 2rpx solid #ccc;
  border-radius: 20rpx;
  overflow: hidden;
}
.navigate-container {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color:#6e6e6e;
  background: #f5f5f5;
  display: flex;
  justify-content: space-around;
}
.navigate-container view {
  width: 120rpx;
  text-align: center;
}
.navigate-container image {
  width: 30rpx;
  height: 30rpx;
}
.active {
  color:orangered;
}
.warn-container{
  width: 100%;
  text-align: center;
  font-size: 40rpx;
  line-height: 100rpx;
  color:red;
  background:#f5f5f5;
  margin-top:10rpx; 
}
